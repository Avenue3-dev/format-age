    "@typescript-eslint/parser": "^7.2.0",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
