  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avenue3-dev/format-age.git"
  },
  "license": "MIT",
  "description": "A utility for formatting age",
  "private": false,
  "scripts": {
    "prepare": "husky",
    "release": "standard-version",
    "build": "rimraf dist/ && tsc --project tsconfig.build.json",
